<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controler.fragments.GenerateCurrentLocalisationFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <TextView
            android:id="@+id/fragment_generate_current_localisation_lattitude"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="lattitude" />

        <TextView
            android:id="@+id/fragment_generate_current_localisation_longitude"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="longitude" />

        <TextView
            android:id="@+id/fragment_generate_current_localisation_adress"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="adresse" />

        <Button
            android:id="@+id/fragment_generate_current_localisation_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Get Location" />


    </LinearLayout>




</FrameLayout>